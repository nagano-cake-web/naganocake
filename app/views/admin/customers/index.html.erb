<h1>会員一覧</h1>
<div>
  <table class="table">
    <thead>
      <tr>
        <th scope="col">会員ID</th>
        <th scope="col">氏名</th>
        <th scope="col">メールアドレス</th>
        <th scope="col">ステータス</th>
      </tr>
    </thead>
    <tbody>
      <%= @customer.each do |customer| %>
      <tr>
        <td><%= customerid %></td>
        <td><%= link_to customer.last_name, admin_customer_path(customer.id) %>
            <%= link_to customer.first_name, admin_customer_path(customer.id) %></td>
        <td><%= customer.email %></td>
        <td><%= customer.is_active %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>